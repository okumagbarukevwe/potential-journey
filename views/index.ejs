    <script>
        function limitWords(event) {
          let textarea = event.target;
          let words = textarea.value.trim().split(/\s+/); // split by spaces
          if (words.length > 12) {
            // keep only the first 12 words
            textarea.value = words.slice(0, 12).join(" ");
            alert("Limit reached: maximum 12 words allowed.");
          }
        }
      </script>
    <style>
        *{
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }
        body{
            font-family: sans-serif;
            background-color: #F0F2F7;
        }
        .headBox{
            position: fixed;
            top: 0;
            width: 100%;
            padding: 20px;
            background: linear-gradient(90deg, rgb(255, 1, 150) 0%, rgb(124, 51, 185) 100%);
            display: flex;
            align-items: center;
            justify-content: space-between;
        }
        .headTxt{
            display: flex;
            align-items: center;
        }
        .txtHead span{
            color: #fff;
            font-size: 14px;
            font-weight: 600;
        }
        .btnBox{
            margin-left: 10px;
        }
        .btnBox button{
            background-color: transparent;
            color: #fff;
            border: 1px solid #fff;
            padding: 10px 15px;
            border-radius: 100px;
            font-family: inherit;
            font-size: 14px;
            font-weight: 500;
            cursor: pointer;
        }
        .mainPrt{
            width: 48%;
            background-color: #fff;
            padding: 40px;
            margin: 0 auto;
            margin-top: 130px;
            border-radius: 10px;
        }
        .keyBox{
            width: 15%;
            margin: 10px auto;
            background-color: rgb(240, 242, 247);
            text-align: center;
            padding: 15px 2px;
            border-radius: 100px;
        }
        .txtMainBox{
             padding: 20px 60px;
             text-align: center;
        }
        .txtMainBox h2{
           color: rgb(18, 29, 51);
           font-size: 20px;
           font-weight: 600;
           margin: 15px 0;
           line-height: 28px;
        }
        .txtMainBox p{
            color: rgb(103, 113, 132);
            line-height: 24px;
            font-weight: 500;
        }
        .areaInp{
            text-align: center;
        }
        .fboxMain textarea {
            background-color: rgb(240, 242, 247);
            width: calc(100% - 48px);
            border-radius: 16px;
            padding: 12px 16px;
            min-height: 120px;
            resize: none;
            font-size: 16px;
            border: 1px solid transparent;
            font-family: inherit;
            font-weight: 500;
        }
        .fboxMain textarea:focus{
            border: 1px solid rgb(12, 108, 242);
            outline: none;
        }
        .pBt{
            margin: 5px 0;
            margin-left: 25px;
            font-size: 12px;
            color: rgb(103, 113, 132);
        }
        .btnSendBox {
           margin-top: 50px;
            padding: 10px 30px;
        }
        .btnSendBox button{
            width: 100%;
            background-color: rgb(12, 108, 242);
            padding: 17px 15px;
            font-family: sans-serif;
            font-size: 15px;
            border-radius: 100px;
            color: #fff;
            font-weight: 600;
            border: none;
            cursor: pointer;
            
        }
        .btnSendBox button:disabled{
            background-color: rgb(101, 165, 255);
            cursor: not-allowed;
        }
        .footBox{
            text-align: center;
            margin: 30px 0;
        }
        .footBox p{
            font-size: 12px;
            color: rgb(103, 113, 132);
        }
        .headLg2{
            display: none;
        }

        .loaderBox{
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100vh;
            background-color: #ffffffd2;
        
        }
        .loadBox{
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            text-align: center;
        }
        .loader,
        .loader:after {
        border-radius: 50%;
        width: 10em;
        height: 10em;

        }
        .loader {
        font-size: 7px;
        position: relative;
        text-indent: -9999em;
        border-top: 1.4em solid rgb(12, 108, 242);
        border-right: 1.4em solid rgb(12, 108, 242);
        border-bottom: 1.4em solid rgb(12, 108, 242);
        border-left: 1.4em solid #ccc;
        -webkit-transform: translateZ(0);
        -ms-transform: translateZ(0);
        transform: translateZ(0);
        -webkit-animation: load8 1.1s infinite linear;
        animation: load8 1.1s infinite linear;
        overflow: hidden;
        }
        @-webkit-keyframes load8 {
        0% {
            -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
        }
        100% {
            -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
        }
        }
        @keyframes load8 {
        0% {
            -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
        }
        100% {
            -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
        }
        }
        .loadBox{
           display: flex;
           flex-direction: column;
           align-items: center;
           justify-content: center;
        }
        .depText h2 {
            font-weight: 600;
            font-size: 19px;
            margin: 10px 0;
            color: #292727;
        }
        .hide {
            display: none;
        }
        .error{
            color: rgb(219, 20, 20) !important;
        }
        .valid{
            color: rgb(10, 160, 10);
        }
        @media screen and (max-width: 600px){
            .headLg{
                display: none;
            }
            .headLg2{
                display: block;
            }
            .txtHead{
                display: none;
            }
            .headBox{
                padding: 10px 15px;
            }
            .btnBox button{
                font-size: 12px;
                padding: 10px;
                font-weight: 600;
                border-radius: 50px;
            }
            .mainPrt{
                width: 95%;
                margin-top: 30px;
                z-index: 3000;
                padding: 40px 10px;
            }
            .txtMainBox{
                padding: 10px 15px;
            }
            .txtMainBox p{
                font-size: 15px;
                line-height: 22px;
            }
            .areaInp textarea {
                font-size: 15px;
            }
            .keyBox{
                width: 25%;
            }
              .depText h2{
                font-size: 16px;
              }
            
        }
    </style>
